---
layout: default.njk
---
<ul class="tags">
{%- for tag in tags | relevant -%}
<li><a href="/dikt/tags/{{ tag | slugify }}/">{{ tag }}</a></li>
{%- endfor -%}
</ul>
{%- if title %}  
<h1>{{ title }}</h1>
{%- endif %}

{{ content | safe }}

{%- if originaltitle %}
<hr/>
Etter <em>{{ originaltitle }}</em> - {{ author }}
{%- endif %}